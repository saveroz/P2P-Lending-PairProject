<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
  <script src="https://kit.fontawesome.com/d00c61de42.js"></script>
  <!-- <link rel="stylesheet" href="./user.css"> -->
  <title>User</title>
</head>
<body>

  <div class="jumbotron">
    <div class="ava-img">
      <img src="" alt="ava1">
    </div>
  </div>

  <section id="history-order">
    <div class="container">
      <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Status</th>
            <th scope="col">Bunga</th>
            <th scope="col">Waktu</th>
            <th scope="col">Nilai</th>
            <th scope="col">Nama</th>
            <th scope="col">Progress</th>
            <th scope="col">Sisa Nilai/Hari</th>
            <th scope="col">Options</th>
          </tr>
        </thead>
        <tbody>
            <% borrower.forEach((borrower) => { %>
              <tr>
                <td scope="col"><%= borrower.id %></td>
                <td scope="col"><%= borrower.status%></td>
                <td scope="col"><%= borrower.bunga %></td>
                <td scope="col"><%= borrower.deadline %></td>
                <td scope="col"><%= borrower.BorrowedMoney%></td>
                <td scope="col"><a href="/admin/listPeminjam/<%= borrower.id%>"><%= borrower.name%></a></td>
                <td scope="col"></td>
                <td scope="col"><%= borrower.dataValues.sisahari%> days</td>
                <td scope="col"><a href="/admin/edit/<%= borrower.id%>" >Edit</a>  <a href="/admin/delete/<%= borrower.id%>" >Delete</a></td>
              </tr>
            <% }) %>
        </tbody>
      </table> 
    </div>
  </section>
</body>
</html>